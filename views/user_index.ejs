<html>
<body>
    <div class="container">
        <h2><%= title %></h2>
        <table class="table table-dark table-striped">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Phase</th>
                    <th>Due Date</th>
                    <th>Assigned To</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody>
                <% user.forEach(function(user){ %>
                    <tr>
                        <td><%= user.name %></td>
                        <td><%= user.phase %></td>
                        <td><%= user.dueDate %></td>
                        <td><%= user.assignedTo %></td>
                        <td>
                            <button class="btn btn-sm btn-primary">Edit</button>
                            <button class="btn btn-sm btn-danger">Delete</button>
                        </td>
                    </tr>
                <% }); %>

            </tbody>
        </table>
    </div>
</body>
</html>